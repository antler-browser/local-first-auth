{
  "name": "irl-browser-onboarding",
  "version": "1.0.0",
  "description": "Onboarding UI for IRL Browser mini-apps - download app or create one-time account",
  "author": "Daniel Mathews <danny@antlerbrowser.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "dev:example": "cd example && npm install && npm run dev",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@stablelib/ed25519": "^1.0.3",
    "base58-universal": "^2.0.0",
    "base64-js": "^1.5.1"
  },
  "devDependencies": {
    "@types/base64-js": "^1.3.2",
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "keywords": [
    "irl-browser",
    "antler",
    "onboarding",
    "authentication",
    "did",
    "decentralized-identity",
    "mini-app"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/antler/irl-browser-onboarding"
  }
}
